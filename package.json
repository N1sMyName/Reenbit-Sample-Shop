{
  "name": "reenbit-sample-shop",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "predeploy": "ng build --base-href https://N1sMyName.github.io/Reenbit-Sample-Shop/",
    "deploy": "ngh --dir=dist/Reenbit-Sample-Shop",
    "ghP": "concurrently \"npm run predeploy\" \"npm run deploy\"",
